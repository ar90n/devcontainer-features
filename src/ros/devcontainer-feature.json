{
    "id": "ros",
    "version": "1.0.0",
    "name": "ROS",
    "documentationURL": "",
    "description": "Installs ROS.",
    "options": {
        "distro": {
            "type": "string",
            "proposals": [
                "noetic"
            ],
            "default": "noetic",
            "description": "Select or enter a ROS distro to install"
        }
    },
    "containerEnv": {
        "ROS_IP": "127.0.0.1",
        "ROS_MASTER_URI": "http://127.0.0.1:11311"
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
